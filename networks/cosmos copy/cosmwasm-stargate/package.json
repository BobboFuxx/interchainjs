{
  "name": "@cosmonauts/cosmos-cosmwasm-stargate",
  "version": "1.0.2",
  "description": "Cosmos sign and broadcast instances",
  "author": "liujun93 <ljun8575@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "proto:download": "rm -rf ./proto && ./scripts/download-proto.sh",
    "proto:codegen": "rm -rf ./src/codegen ./.telescope.json && npx telescope transpile --config ../../.telescope.json --config ./.telescope.ext.json",
    "proto:update": "yarn proto:download && yarn proto:codegen",
    "clear": "rimraf dist"
  },
  "publishConfig": {
    "access": "public"
  },
  "imports": {
    ".": {
      "types": "./cjs/index.d.ts",
      "import": "./esm/index.js",
      "require": "./cjs/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "dependencies": {
    "@cosmonauts/cosmos-stargate": "^1.0.0"
  },
  "peerDependencies": {
    "@cosmonauts/core": "^1.0.0",
    "@cosmonauts/cosmos-amino": "^1.0.0",
    "@cosmonauts/cosmos-cosmjs": "^1.0.0",
    "@cosmonauts/cosmos-proto": "^1.0.0"
  },
  "devDependencies": {
    "@cosmology/telescope": "^1.4.3",
    "axios": "^1.5.1",
    "rimraf": "^5.0.1"
  }
}
