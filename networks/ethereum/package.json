  "license": "MIT",
  "scripts": {
    "rename": "renamer -f \"/^package.dist.json$/\" -r \"package.json\" \"dist/*\"",
    "copy": "copyfiles package.dist.json README.md dist",
    "copy-and-rename": "npm run copy && npm run rename",
    "clean": "rimraf dist",
    "build": "npm run clean; tsc; tsc -p tsconfig.esm.json; npm run copy-and-rename",
    "pub": "cd dist && npm run pub"
  },
  "dependencies": {
    "@cosmonauts/utils": "^1.0.0",
    "@cosmonauts/types": "^1.0.0",
    "@ethersproject/transactions": "^5.7.0",
    "@ethersproject/bignumber": "^5.7.0",
    "@ethersproject/bytes": "^5.7.0",
    "@ethersproject/hash": "^5.7.0",
    "@noble/hashes": "^1.3.1"
  }
}