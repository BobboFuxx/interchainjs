  "scripts": {
    "rename": "renamer -f \"/^package.dist.json$/\" -r \"package.json\" \"dist/*\"",
    "copy": "copyfiles package.dist.json README.md dist",
    "copy-and-rename": "npm run copy && npm run rename",
    "clean": "rimraf dist",
    "build": "npm run clean; tsc; npm run copy-and-rename",
    "pub": "cd dist && npm run pub"
  },
  "dependencies": {
    "decimal.js": "^10.4.3",
    "@cosmonauts/utils": "^1.0.0"
  }
}