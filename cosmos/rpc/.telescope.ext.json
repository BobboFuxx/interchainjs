{
  "protoDirs": ["./proto"],
  "outPath": "./src/codegen",
  "options": {
    "interfaces": {
      "enabled": false
    },
    "prototypes": {
      "methods": {
        "toAmino": false,
        "fromAmino": false
      }
    },
    "addAminoTypeToObjects": false,
    "aminoEncoding": {
      "enabled": false,
      "useProtoOptionality": true
    },
    "rpcClients": {
      "serviceImplement": {
        "Query": {
          "type": "Query"
        }
      },
      "instantOps": [
        {
          "className": "QueryImpl",
          "include": {
            "patterns": [
              "cosmos.auth.v1beta1.account",
              "cosmos.tx.v1beta1.simulate"
            ]
          }
        }
      ]
    }
  }
}
