{
  "protoDirs": ["./proto"],
  "outPath": "./src/codegen",
  "options": {
    "interfaces": {
      "enabled": false
    },
    "prototypes": {
      "methods": {
        "toAmino": false,
        "fromAmino": false
      }
    },
    "addAminoTypeToObjects": false,
    "aminoEncoding": {
      "enabled": false,
      "useProtoOptionality": true
    },
    "rpcClients": {
      "serviceImplement": {
        "Query": {
          "type": "Query"
        }
      },
      "instantOps": [
        {
          "className": "QueryImpl",
          "include": {
            "patterns": [
              "cosmos.auth.v1beta1.*",
              "cosmos.bank.v1beta1.*",
              "cosmos.gov.v1beta1.*",
              "cosmos.staking.v1beta1.*",
              "cosmwasm.wasm.v1.*",
              "cosmos.tx.v1beta1.simulate"
            ]
          },
          "nameMapping": {
            "Query": {
              "authParams": "cosmos.auth.v1beta1.params",
              "bankParams": "cosmos.bank.v1beta1.params",
              "govParams": "cosmos.gov.v1beta1.params",
              "stakingParams": "cosmos.staking.v1beta1.params",
              "wasmParams": "cosmwasm.wasm.v1.params"
            }
          }
        }
      ]
    }
  }
}
