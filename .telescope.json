{
  "protoDirs": ["./proto"],
  "outPath": "./src/codegen",
  "options": {
    "env": "v-next",
    "fieldDefaultIsOptional": false,
    "useSDKTypes": false,
    "useOptionalNullable": true,
    "interfaces": {
      "enabled": false,
      "useUnionTypes": false
    },
    "prototypes": {
      "enableRegistryLoader": false,
      "enableMessageComposer": false,
      "enabled": true,
      "parser": {
        "keepCase": false
      },
      "methods": {
        "fromJSON": true,
        "toJSON": true,
        "encode": true,
        "decode": true,
        "fromPartial": true,
        "toAmino": true,
        "fromAmino": true,
        "fromProto": true,
        "toProto": true
      },
      "addTypeUrlToDecoders": false,
      "addTypeUrlToObjects": true,
      "addAminoTypeToObjects": true,
      "typingsFormat": {
        "duration": "duration",
        "timestamp": "date",
        "useExact": false,
        "useDeepPartial": true,
        "num64": "bigint",
        "customTypes": {
          "useCosmosSDKDec": false
        },
        "useTelescopeGeneratedType": true
      }
    },
    "bundle": {
      "enabled": false
    },
    "stargateClients": {
      "enabled": false,
      "includeCosmosDefaultTypes": true
    },
    "aminoEncoding": {
      "enabled": true,
      "useLegacyInlineEncoding": false,
      "disableMsgTypes": true
    },
    "lcdClients": {
      "enabled": false
    },
    "rpcClients": {
      "enabled": true,
      "inline": false,
      "extensions": true,
      "camelCase": true,
      "enabledServices": ["Query"]
    }
  }
}
