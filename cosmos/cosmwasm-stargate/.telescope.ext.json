{
  "protoDirs": ["./proto"],
  "outPath": "./src/codegen",
  "options": {
    "interfaces": {
      "enabled": true,
      "useGlobalDecoderRegistry": true,
      "useUnionTypes": true
    },
    "prototypes": {
      "methods": {
        "toAmino": true,
        "fromAmino": true,
        "toAminoMsg": true,
        "fromAminoMsg": true
      }
    },
    "aminoEncoding": {
      "enabled": true,
      "useLegacyInlineEncoding": false,
      "disableMsgTypes": false,
      "useProtoOptionality": true
    },
    "rpcClients": {
      "serviceImplement": {
        "Msg": {
          "type": "Tx"
        },
        "Query": {
          "type": "Query"
        }
      },
      "instantOps": [
        {
          "className": "CosmWasmImpl",
          "include": {
            "patterns": ["cosmwasm.wasm.v1.*"]
          },
          "nameMapping": {
            "Msg": {
              "updateWasmParams": "cosmwasm.wasm.v1.updateParams"
            },
            "Query": {
              "getWasmParams": "cosmwasm.wasm.v1.params"
            }
          }
        }
      ]
    }
  }
}
